{
    "name": "Levya-Org/levya-identity",
    "description": "Levya Identity Platform",
    "keywords": ["levya", "identity", "php"],
    "homepage": "https://levya.org/",
    "type": "project",
    "license": "GPL-2.0+",
    "support": {
        "issues": "https://github.com/Levya-Org/levya-identity/issues?state=open",
        "forum": "https://forum.levya.org/",
        "wiki": "https://wiki.levya.org/",
        "source": "https://github.com/Levya-Org/levya-identity"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.5.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
		"yiisoft/yii2-jui": "*",
        "zendframework/zend-ldap": "^2.7",
        "kartik-v/yii2-password": "@dev",
        "kartik-v/yii2-helpers": "@dev",
        "kartik-v/yii2-widget-datepicker": "@dev",
        "kartik-v/bootstrap-tabs-x": "@dev",
        "michelf/php-smartypants": "dev-lib",
        "kartik-v/yii2-markdown": "@dev",
        "kartik-v/yii2-tabs-x": "@dev",
        "ramsey/uuid": "^3.0",
        "nonzod/yii2-zend-ldap": ">=0.0.7"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
